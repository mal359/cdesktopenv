ACTION k3b
{
     LABEL         k3b
     TYPE          COMMAND
     EXEC_STRING   k3b %Arg_1%
     ICON          k3b
     WINDOW_TYPE   NO_STDIO
     DESCRIPTION   k3b Burn CDs and DVDs.
}

