ACTION mgv
{
     LABEL         mgv
     TYPE          COMMAND
     EXEC_STRING   mgv "%Arg_1%"
     ICON          mgv
     WINDOW_TYPE   NO_STDIO
     DESCRIPTION   mgv, the Motif version of gv, is the default ps viewer.
}

